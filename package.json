{
  "name": "eucalypt",
  "version": "0.0.1",
  "description": "Reagent compatibale-ish Squint-cljs library with no React. Builds tiny frontends.",
  "type": "module",
  "scripts": {
    "build": "npm run demo:build",
    "dev": "npm run demo:dev",
    "test": "npm run test:compile && vitest run --config test/vitest.config.js",
    "test:watch": "npm run test:compile && vitest --config test/vitest.config.js",
    "demo:build": "squint compile --config demo/squint.edn && vite build demo",
    "demo:watch": "squint watch --config demo/squint.edn",
    "demo:dev": "concurrently \"npm run demo:watch\" \"vite demo --port 8000\"",
    "test:compile": "squint compile --config test/squint.edn"
  },
  "devDependencies": {
    "concurrently": "8.2.2",
    "es-toolkit": "1.39.10",
    "happy-dom": "14.12.3",
    "squint-cljs": "latest",
    "vite": "7.1.2",
    "vite-plugin-singlefile": "2.3.0",
    "vitest": "1.6.0"
  }
}
